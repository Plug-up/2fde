ar: 
	@mkdir -p ./ar
	@for f in internat.po internat.mo; do \
	if test ! -f ./ar/$$f -a ! -d ./ar/$$f ; \
	then x=yep ; \
	else x=`find $(srcdir)/ar/$$f -newer ./ar/$$f -print` ; \
	fi; \
	case "$$x" in ?*) \
	cp -pRf $(srcdir)/ar/$$f ./ar ;; \
	esac; \
	done

